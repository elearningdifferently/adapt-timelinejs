<div class="timelinejs-inner component-inner">
    {{> component this}}


<div id='timeline-embed' style="width: 100%; height: 600px"></div>


<script type="text/javascript">
                   $(document).ready(function(){
          var sammy = { 
              

    "title": {
        "media": {
          "url": "{{#if _media.image}}{{_media.image}}{{else}}{{_media.source}}{{/if}}",
          "caption": "{{#if _media.caption}}{{_media.caption}}{{else}}{{/if}}",
          "credit": "{{#if _media.credit}}{{_media.credit}}{{else}}{{/if}}"
        },
        "text": {
          "headline": "{{{timelineheadline}}}",
          "text": "{{{timelinetext}}}"
        }
    },
    "events": [
                                                 {{#each _items}}
      {
        "media": {
          "url": "{{#if _event_media.image}}{{_event_media.image}}{{else}}{{_event_media.source}}{{/if}}",
          "caption": "{{#if _event_media.caption}}{{_event_media.caption}}{{else}}{{/if}}",
          "credit": "{{#if _event_media.credit}}{{_event_media.credit}}{{else}}{{/if}}"
        },
        "start_date": {
          "month": "{{#if _event_month}}{{_event_month}}{{else}}{{/if}}",
          "day": "{{#if _event_day}}{{_event_day}}{{else}}{{/if}}",
          "year": "{{#if _event_year}}{{_event_year}}{{else}}{{/if}}"
        },
        "text": {
          "headline": "{{{event_title}}}",
          "text": "{{#if event_description}}{{{event_description}}}{{else}}{{/if}}"
        }
      },
                                            {{/each}}
    ]


}
          // replace make_the_json() with the JSON object you created
          // two arguments: the id of the Timeline container (no '#')
          // and the JSON object or an instance of TL.TimelineConfig created from
          // a suitable JSON object
          window.timeline = new TL.Timeline('timeline-embed', sammy);
                   });
        </script>


</div>



